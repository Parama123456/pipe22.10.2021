pipeline(){
 agent any
    stages{
       stage ('docker image ls'){
          steps {
              sh "docker image ls"
               }
                             }
                             stage('docker container'){
                             steps {
                             sh "docker ps -a "
                             }
                             }
                             stages('create image'){
                             steps {
                             sh "docker build -t nn_image ."
                             }
                             }

          }

}
